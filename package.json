{
  "name": "simple-pos",
  "version": "1.17.0",
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "npm": "please-use-pnpm",
    "pnpm": ">=10"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "ng": "nx",
    "start": "nx serve pos",
    "build": "nx build pos",
    "watch": "nx build pos --watch",
    "test": "nx run-many -t test --passWithNoTests",
    "test:ci": "nx run-many -t test --run --passWithNoTests",
    "analyze": "./scripts/analyze-bundle.sh",
    "tauri": "tauri",
    "tauri:dev": "nx dev native",
    "tauri:build": "nx build native",
    "format": "nx format:write",
    "lint": "nx lint pos",
    "db:reset": "node scripts/reset-db.js",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{html,ts,json,md,css,scss}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ],
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@angular/service-worker": "^21.1.0",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-log": "^2.8.0",
    "@tauri-apps/plugin-process": "^2.3.1",
    "@tauri-apps/plugin-sql": "^2.3.1",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "bcryptjs": "^3.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^2.2.3",
    "@angular-devkit/core": "^21.1.2",
    "@angular-devkit/schematics": "^21.1.2",
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.0",
    "@angular/platform-browser-dynamic": "^21.1.2",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "22.4.5",
    "@nx/eslint": "22.4.5",
    "@nx/eslint-plugin": "22.4.5",
    "@nx/js": "^22.4.5",
    "@nx/vite": "^22.4.5",
    "@nx/vitest": "22.4.5",
    "@nx/web": "^22.4.5",
    "@nx/workspace": "22.4.5",
    "@schematics/angular": "^21.1.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@tauri-apps/cli": "^2.9.6",
    "@types/node": "20.19.9",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "fake-indexeddb": "^6.2.5",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.2.7",
    "nx": "22.4.5",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "semantic-release": "^25.0.3",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.1.0",
    "vitest": "^4.0.8"
  }
}
