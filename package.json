{
  "name": "simple-pos",
  "version": "1.23.1",
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "npm": "please-use-pnpm",
    "pnpm": ">=10"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "ng": "nx",
    "dev": "nx run-many -t serve --projects=pos,api --parallel",
    "dev:pos": "nx serve pos",
    "dev:api": "nx serve api",
    "build": "nx run-many -t build",
    "build:api": "nx build api",
    "build:pos": "nx build pos",
    "test": "nx run-many -t test --passWithNoTests",
    "test:api": "nx test api",
    "test:pos": "nx test pos",
    "test:ci": "nx run-many -t test --run --passWithNoTests",
    "test:ci:coverage": "nx run-many -t test --coverage --run --passWithNoTests",
    "lint": "nx run-many -t lint",
    "lint:api": "nx lint api",
    "lint:pos": "nx lint pos",
    "format": "nx format:write",
    "db:reset": "node scripts/reset-db.js",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{html,ts,json,md,css,scss}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ],
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.1.3",
    "@angular/compiler": "^21.1.3",
    "@angular/core": "^21.1.3",
    "@angular/forms": "^21.1.3",
    "@angular/platform-browser": "^21.1.3",
    "@angular/router": "^21.1.3",
    "@angular/service-worker": "^21.1.3",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.0",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "bcryptjs": "^3.0.3",
    "express": "^5.2.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.18.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.8.0",
    "stripe": "^20.3.1",
    "tslib": "^2.3.0",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^2.2.3",
    "@angular-devkit/core": "^21.1.3",
    "@angular-devkit/schematics": "^21.1.3",
    "@angular/build": "^21.1.3",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^21.1.3",
    "@angular/platform-browser-dynamic": "^21.1.3",
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.13",
    "@nx/angular": "22.4.5",
    "@nx/eslint": "22.4.5",
    "@nx/eslint-plugin": "22.4.5",
    "@nx/jest": "22.4.5",
    "@nx/js": "^22.4.5",
    "@nx/nest": "^22.4.5",
    "@nx/node": "22.4.5",
    "@nx/vite": "^22.4.5",
    "@nx/vitest": "22.4.5",
    "@nx/web": "^22.4.5",
    "@nx/webpack": "22.4.5",
    "@nx/workspace": "22.4.5",
    "@schematics/angular": "^21.1.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@swc-node/register": "~1.11.1",
    "@swc/core": "~1.15.11",
    "@swc/helpers": "~0.5.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.12",
    "@types/node": "20.19.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.16.0",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.24",
    "axios": "^1.6.0",
    "dotenv": "^17.2.4",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "fake-indexeddb": "^6.2.5",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.2.7",
    "nx": "22.4.5",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prisma": "^7.3.0",
    "semantic-release": "^25.0.3",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.1.2",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.55.0",
    "unplugin-swc": "^1.5.9",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.1.0",
    "vitest": "^4.0.8",
    "webpack-cli": "^5.1.4"
  }
}
