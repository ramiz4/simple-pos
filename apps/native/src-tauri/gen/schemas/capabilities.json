{
  "default": {
    "identifier": "default",
    "description": "Default capability for the main window",
    "local": true,
    "windows": ["main"],
    "permissions": [
      "core:default",
      "core:event:default",
      "core:window:default",
      "sql:default",
      "sql:allow-load",
      "sql:allow-execute",
      "sql:allow-select",
      "sql:allow-close",
      "log:default",
      "core:app:allow-version"
    ]
  }
}
